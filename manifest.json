{
    "name": "Cryptodog",
    "description": "Private conversations made easy",
    "version": "2.5.8",
    "manifest_version": 2,
    "background": {
        "scripts": ["background/background.js"]
    },
    "browser_action": {
        "default_icon": {
            "128": "img/logo-128.png"
        }
    },
    "icons": {
        "128": "img/logo-128.png"
    },
    "permissions": [
        "activeTab",
	    "notifications",
        "storage"
    ]
}
